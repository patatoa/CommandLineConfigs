#!/bin/sh

 pulseaudio-ctl $1  > /dev/null
	 pulseaudio-ctl full-status |
	 awk '{
	  if ($2 =="yes")
		  print "ðŸ”‡mute"
	  else {
		  if($1 < 35) emoji="ðŸ”ˆ"
		  else if($1 < 65) emoji="ðŸ”‰"
		  else emoji="ðŸ”Š"

		  print emoji $1 "%"
	  }
	  }' > ~/.local/.currentVolume
	 pkill -RTMIN+1 dwmblocks
